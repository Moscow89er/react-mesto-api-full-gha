older=\"Имя\"\r\n                className=\"popup__input popup__input_type_username\"\r\n                minLength=\"2\"\r\n                maxLength=\"40\"\r\n                required\r\n            />\r\n            {!isValid && formValues.username && <div className=\"popup__form-error-username\">{formErrors.username}</div>}\r\n            <input\r\n                onChange={(evt) => {\r\n                    handleDescriptionChange(evt);\r\n                    handleInputChange(evt);\r\n                }}\r\n                value={description || ''}\r\n                id=\"about\"\r\n                type=\"text\"\r\n                name=\"about\"\r\n                placeholder=\"Вид деятельности\"\r\n                className=\"popup__input popup__input_type_about\"\r\n                minLength=\"2\"\r\n                maxLength=\"200\"\r\n                required\r\n            />\r\n            {!isValid && formValues.about && <div className=\"popup__form-error-about\">{formErrors.about}</div>}\r\n        </PopupWithForm>\r\n    );\r\n}\r\n\r\nexport default EditProfilePopup;","C:\\Users\\Batumi_89er\\dev\\mesto-react\\src\\components\\EditAvatarPopup.js",[],[],"C:\\Users\\Batumi_89er\\dev\\mesto-react\\src\\components\\AddPlacePopup.js",[],[],"C:\\Users\\Batumi_89er\\dev\\mesto-react\\src\\components\\ConfirmButtonPopup.js",[],[],"C:\\Users\\Batumi_89er\\dev\\mesto-react\\src\\utils\\useFormValidator.js",[],[],"C:\\Users\\Batumi_89er\\dev\\react-mesto-auth\\src\\index.js",[],[],[],"C:\\Users\\Batumi_89er\\dev\\react-mesto-auth\\src\\components\\App.js",["182"],[],"C:\\Users\\Batumi_89er\\dev\\react-mesto-auth\\src\\components\\Footer.js",[],[],"C:\\Users\\Batumi_89er\\dev\\react-mesto-auth\\src\\components\\Main.js",[],[],"C:\\Users\\Batumi_89er\\dev\\react-mesto-auth\\src\\components\\Header.js",[],[],"C:\\Users\\Batumi_89er\\dev\\react-mesto-auth\\src\\components\\EditProfilePopup.js",[],[],"C:\\Users\\Batumi_89er\\dev\\react-mesto-auth\\src\\components\\Ima